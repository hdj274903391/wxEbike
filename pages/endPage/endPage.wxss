.container{
  width: 100hv;
  height: 100%;
  border-top: 1px solid #cdcdcd;
}
.title{
  font-size: 15px;
  white-space: 8px;
  margin-top: 65px;
}
.titlePrice{
  font-size: 18px;
  font-weight: 600;
}
.detail{
  font-size: 15px;
  margin-top: 20px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.detailNum{
  font-size: 18px;
  font-weight: 600;
}
.endView{
  width: 180px;
  height: 40px;
  border-radius: 40px;
  text-align: center;
  line-height: 40px;
  font-size: 18px;
  background: #5ac72f;
  color: white;
  margin-top: 60px;
}
.introduce{
  font-size:15px;
  margin-top: 40px;
  margin-bottom: 12px; 
  color: #999;
}
.lock{
  width: 180px;
  height: 40px;
  border-radius: 40px;
  text-align: center;
  line-height: 40px;
  font-size: 18px;
  border: 1px solid #999;
  color: #646464;
}
