.container{
  width: 100%;
  height: 100vh;
  border-top: 1px solid #f0f0f0;
  display: flex;
 align-items: center;
  flex-direction: column;
  background: #f0f0f0;
  justify-content: flex-start;
}
.contentTop{
  width: 100%;
  display: flex;
  flex-direction: column;
 align-items: flex-end;
  background: white;
}
.list{width: 96%;
  border-bottom:1px solid #f0f0f0;
  height: 60px;font-size: 15px;
  line-height: 60px;
  display: flex;
  align-items: center;
  justify-content: space-between;

}
.nullView{
  height: 4px;
  width: 100%;
  background: #f0f0f0;
}
.list>text{
  color: #464646;
}
.detail{width: 6px;
  height: 6px;
  border-top: 1px solid #999;
  border-right: 1px solid #999;
  transform: rotate(45deg);
  margin-right:15px;
}
.out{width: 180px;
  height: 40px;
  border: 1px solid #5ac72f;
  text-align: center;
  line-height: 40px;  
  border-radius: 40px;
  color: #5ac72f;
  background: transparent;
}
.bottom{
 position: absolute;
  bottom: 50rpx;
  display: flex;
  justify-content: center;
  width: 100%;
}
