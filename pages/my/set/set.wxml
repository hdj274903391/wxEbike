<!--pages/set/set.wxml-->
<view class='container'>
  <view class='contentTop'>
    <view class='list' >
      <text>操作流程</text>
      <view class='detail'></view>
    </view>
    <view class='list' style='border-bottom:0px;'>
      <text>关于我们</text>
      <view class='detail'></view>
    </view>
    <view class='nullView'></view>
    <view class='list'  bindtap='handleToRule'  data-rule="priceRule" data-navigation="计费规则">
    <text>计费规则</text>
    <view class='detail'></view>
    </view>
    <view class='list'>
    <text>充值协议</text>
    <view class='detail'></view>
    </view>
    <view class='list'  bindtap='handleToRule' data-rule="loginRule" data-navigation="用户服务协议">
    <text>用户服务协议</text>
    <view class='detail'></view>
    </view>
    <view class='list'>
    <text>用户积分规则</text>
    <view class='detail'></view>
    </view>
    <view class='list'bindtap='aboutService'>
    <text  >客服相关</text>
    <view class='detail'></view>
    </view>
  </view>
  <view class='bottom'>
    <button  open-type='getUserInfo' class='out' bindtap='handleGoOut'>{{bottomContent}}</button>
  </view>
  
</view>
