.content{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
 
}
#MapDetail{
  width: 100%;
  height: 200px;
}
.travelDetail{
 display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 30px;
}
.travelDetail>text{
 font-size: 15px; 
}
.price{
  font-size: 18px;
}
.travelNum{
  padding: 30px 0;
  width: 87%;
  display: flex;
  height: 20px;
  justify-content: space-around;
  border-bottom: 1px solid #f0f0f0;
  align-items: center;
  font-size: 15px;
  white-space: nowrap
}
.travelNum>text{
  font-size: 18px;
  height: 100%;
  line-height: 20px;
}
.travelPriceList{
  height: 40px;
  width: 87%;
  display: flex;
  justify-content: space-between;
  font-size: 15px;
  align-items: center;
}
.share{
  width: 180px;
  height: 43px;
  border-radius: 43px;
  border: 1px solid #95e280;
  text-align: center;
  line-height: 43px;
  color: #95e280;
  position: absolute;
  left: calc(50% - 90px);
  bottom: 40px;
  background: white;
}