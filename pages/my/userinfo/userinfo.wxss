.container{
  width: 100%;
  border-top: 1px solid #f0f0f0;
  padding: 0 ;
  overflow: hidden;
}
.top{
  width: 100%;
  height: 100px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 15px;
}
.top>view:nth-child(1){
  margin-left: 15px;
}
.phone{
  font-size: 16px;
  font-weight: 600;
  height: 30px;
  line-height: 30px;

}
.score{
  font-size: 15px;
}
.userImg{
  width: 60px;
  height: 60px;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 15px;
}
.userImg>image{
  width: 100%;
  height: 100%;
}
.list{width: 100%;
  border-bottom:1px solid #f0f0f0;
  height: 60px;font-size: 15px;
  line-height: 60px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-left: 30px;
}
.list>text{
  color:#464646; 
}
.list>view>text{
  vertical-align: middle;
  margin-right: 10px;
  color:#999 ;
}
.nullView{
  height: 4px;
  width: 100%;
  background: #f0f0f0;
}
.detail{width: 6px;
  height: 6px;
  border-top: 1px solid #999;
  border-right: 1px solid #999;
  transform: rotate(45deg);
  margin-right: 30px;
  display: inline-block;
}
.bottom{ border-bottom: 4px solid #f0f0f0; }
.out{width: 180px;
  height: 40px;
  border: 1px solid #5ac72f;
  text-align: center;
  line-height: 40px;  
  border-radius: 40px;
  color: #5ac72f;
  position: absolute;
  bottom: 36px;
}
