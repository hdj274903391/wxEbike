.container{
  width: 100%;
  padding: 10px;
  border-top: 1px solid #f0f0f0;
}
.containerlist{
 width: 100%;
 height: 210px;
 border-radius: 4px;
 border: 1px solid #f0f0f0;
 overflow: hidden
}
.containerlistimg{
  width: 100%;
  height: 150px;
   background: #dcdcdc;
 
}
.containerlistbottom{
  height: 60px;
  padding: 0 15px;
}
.containerlistbottom>view{
width: 100%;
height: 30px;
display: flex;
align-items: center
}
.containerlistbottom>view:nth-child(1){
  display: flex;
  justify-content: space-between;
}
.title{font-size: 15px;}
.date{
  font-size: 12px;
  color: #999;
  margin-bottom: 10px;
}
.state{margin-top: 10px;border-radius: 4px;background:#5ac72f;font-size: 12px;color: white;width: 62px;height: 20px;text-align: center;line-height: 20px;display: block}